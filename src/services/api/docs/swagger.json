            "get": {
                "description": "Get status of version",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "versions"
                ],
                "summary": "Get status of version",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Version ID",
                        "name": "version_id",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/versions.IsReadyResponse"
                        }
                    }
                }
            }
        },
        "/v1/versions/submit": {
            "post": {
                "description": "Start training process",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "versions"
                ],
                "summary": "Start training process",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Class name",
                        "name": "class",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Payment ID",
                        "name": "id",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "array",
                        "items": {
                            "type": "file"
                        },
                        "description": "Data",
                        "name": "data",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/versions.SubmitDataResponse"
                        }
                    }
                }
            }
        }
