                "url": {
                    "type": "string"
                }
            }
        },
        "prompts.CreatePromptRequest": {
            "type": "object",
            "properties": {
                "amount_images": {
                    "type": "integer"
                },
                "negative_prompt": {
                    "type": "string"
                },
                "prompt": {
                    "type": "string"
                },
                "version_id": {
                    "type": "string"
                }
            }
        },
        "versions.IsReadyResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "is_ready": {
                    "type": "boolean"
                }
            }
        },
        "versions.SubmitDataResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "message": {
