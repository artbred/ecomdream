                },
                "promocode_id": {
                    "type": "string"
                },
                "version_id": {
                    "type": "string"
                }
            }
        },
        "payments.CreatePaymentLinkResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
